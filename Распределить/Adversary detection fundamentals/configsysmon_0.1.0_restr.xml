		<RuleGroup name="Change create time critical file" groupRelation="or">
			<FileCreateTime onmatch="exclude">
				<TargetFilename name="temp path" condition="contains">\temp</TargetFilename>
				<TargetFilename name="temp file" condition="end with">.tmp</TargetFilename>
				<TargetFilename name="temp file" condition="end with">.~tmp</TargetFilename>
				<TargetFilename name="temp file" condition="end with">.temp</TargetFilename>
			</FileCreateTime>			
		</RuleGroup>
		<RuleGroup name="Network Connection is not internet browser and network service" groupRelation="or">
			<NetworkConnect onmatch="exclude">
				<Image name="chrome" condition="end with">\Google\Chrome\Application\chrome.exe</Image><!-->Updated<!-->
				<DestinationIp name="loopback" condition="contains">0:0:0:0:0:0:0:1</DestinationIp>
				<DestinationIp name="loopback" condition="contains">0.0.0.0</DestinationIp>
				<DestinationIp name="loopback" condition="contains">127.0.0.1</DestinationIp>
				<DestinationPortName name="protocol kerberos" condition="contains">kerberos</DestinationPortName>
			</NetworkConnect>
		</RuleGroup>
		<RuleGroup name="Terminate critical process" groupRelation="or">
			<ProcessTerminate onmatch="include">
				<Image name="windows defender" condition="end with">\MsMpEng.exe</Image>
				<Image name="windows defender" condition="contains">Antimalware Service Executable</Image>
				<Image name="PROCESS in hardvaredevice" condition="begin with">\</Image>		
			</ProcessTerminate>			
		</RuleGroup>
